{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "@mln-layouts/*": ["./src/layouts/*"],
      "@mln-images/*": ["./src/assets/images/*"],
      "@mln-svg/*": ["./src/assets/svg/*"],
      "@mln-scss/*": ["./src/assets/scss/*"],
      "@mln-navigator": ["./src/navigator/index","./src/navigator/*"],
      "@mln-redux": ["./src/redux/index", "./src/redux/*"],
      "@mln-thunk": ["./src/thunk"],
      "@mln-components": ["./src/components/*"],
      "@mln-hooks": ["./src/hooks"]
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", "build"]
}
